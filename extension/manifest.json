{
  "name": "Lookuper",
  "description": "A small extension that will help you find and understand new words! Only english for now.",
  "version": "0.0.1",
  "icons": {
    "128": "icons8-search-128.png"
    },
  "manifest_version": 3,
  "action": {
    "default_title": "Next.js app",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"]
}
